class MyBook: public Book{
    int price;
public:
    MyBook(string title, string author, int price): Book(title, author){
        this->price = price;
    }
    
    void display(){
        std::cout<<"Title: "<<title<<std::endl;
        std::cout<<"Author: "<<author<<std::endl;
        std::cout<<"Price: "<<price<<std::endl;
    }
    
};
